{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"class-name": true,
		"comment-format": [
			false,
			"check-space",
			"check-lowercase"
		],
		"ban-types": false,
		"curly": true,
		"eofline": true,
		"indent": [
			true,
			"spaces"
		],
		"interface-name": true,
		"jsdoc-format": true,
		"max-line-length": [
			true,
			400
		],
		"no-any": true,
		"no-arg": true,
		"no-bitwise": false,
		"no-construct": false,
		"no-shadowed-variable": true,
		"no-duplicate-variable": true,
		"no-empty": false,
		"no-eval": true,
		"no-require-imports": false,
		"no-trailing-whitespace": true,
		"no-unused-expression": false,
		"one-variable-per-declaration": false,
		"no-var-keyword": true,
		"no-console": false,
		"prefer-for-of": false,
		"one-line": [
			true,
			"check-catch",
			"check-else",
			"check-open-brace",
			"check-whitespace"
		],
		"quotemark": [
			true,
			"double"
		],
		"radix": false,
		"semicolon": true,
		"triple-equals": [
			true,
			"allow-null-check"
		],
		"variable-name": [
			true,
			"allow-leading-underscore"
		],
		"whitespace": [
			false,
			"check-branch",
			"check-decl",
			"check-operator",
			"check-separator",
			"check-type"
		]
	},
	"rulesDirectory": []
}